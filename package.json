    "moment": "^2.29.4",
    "floating-vue": "^5.2.2",
    "vue": "^3.4.0",
    "vue3-apexcharts": "^1.8.0",
    "vue-axios": "^3.5.2",
    "vue-chartjs": "^5.3.2",
    "vue-cookies": "^1.8.6",
    "vue-material": "^1.0.0", 
